# !! Contents within this block are managed by 'conda init' !!                                                              
__conda_setup="$('/Users/anthony/anaconda3/bin/conda' 'shell.bash' 'hook' 2> /dev/null)"
if [ $? -eq 0 ]; then
    eval "$__conda_setup"
else
    if [ -f "/Users/anthony/anaconda3/etc/profile.d/conda.sh" ]; then
        . "/Users/anthony/anaconda3/etc/profile.d/conda.sh"
    else
        export PATH="/Users/anthony/anaconda3/bin:$PATH"
    fi
fi
unset __conda_setup
